  "auto_tagging_rules": {
    "description": "Reglas para aplicar tags automáticamente basado en metafields",
    "rules": [
      {
        "condition": "metafield_equals",
        "metafield": "custom.property_type",
        "value": "casa",
        "apply_tag": "casa"
      },
      {
        "condition": "metafield_equals",
        "metafield": "custom.property_type",
        "value": "departamento",
        "apply_tag": "departamento"
      },
      {
        "condition": "metafield_equals",
        "metafield": "custom.property_type",
        "value": "terreno",
        "apply_tag": "terreno"
      },
      {
        "condition": "price_range",
        "min_price": 0,
        "max_price": 500000,
        "apply_tag": "precio-0-500000"
      },
      {
        "condition": "price_range",
        "min_price": 500000,
        "max_price": 1000000,
        "apply_tag": "precio-500000-1000000"
      },
      {
        "condition": "price_range",
        "min_price": 1000000,
        "max_price": 2000000,
        "apply_tag": "precio-1000000-2000000"
      },
      {
        "condition": "price_range",
        "min_price": 2000000,
        "max_price": 5000000,
        "apply_tag": "precio-2000000-5000000"
      },
      {
        "condition": "price_range",
        "min_price": 5000000,
        "max_price": null,
        "apply_tag": "precio-5000000-plus"
      },
      {
        "condition": "metafield_equals",
        "metafield": "custom.bedrooms",
        "value": "1",
        "apply_tag": "recamaras-1"
      },
      {
        "condition": "metafield_equals",
        "metafield": "custom.bedrooms",
        "value": "2",
        "apply_tag": "recamaras-2"
      },
      {
        "condition": "metafield_equals",
        "metafield": "custom.bedrooms",
        "value": "3",
        "apply_tag": "recamaras-3"
      },
      {
        "condition": "metafield_equals",
        "metafield": "custom.bedrooms",
        "value": "4",
        "apply_tag": "recamaras-4"
      },
      {
        "condition": "metafield_greater_than",
        "metafield": "custom.bedrooms",
        "value": "4",
        "apply_tag": "recamaras-5-plus"
      }
    ]
  }
